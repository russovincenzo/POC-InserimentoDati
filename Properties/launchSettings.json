{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "/",
      "applicationUrl": "http://localhost:5213",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CONNECTION_STRING": "Server={DB_SERVER_NAME};Database={DB_NAME};User Id={DB_USERNAME};Password={DB_PASSWORD};TrustServerCertificate=True;"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "/",
      "applicationUrl": "https://localhost:7065;http://localhost:5213",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CONNECTION_STRING": "Server={DB_SERVER_NAME};Database={DB_NAME};User Id={DB_USERNAME};Password={DB_PASSWORD};TrustServerCertificate=True;"
      }
    }
  }
}
