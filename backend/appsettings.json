{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Debug",
      "Microsoft.AspNetCore": "Trace",
      "System.Net.Http": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server={DB_SERVER_NAME};Database={DB_NAME};User Id={DB_USERNAME};Password={DB_PASSWORD};TrustServerCertificate=True;"
  }
}